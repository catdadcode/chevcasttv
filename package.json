{
  "name": "chevcasttv",
  "version": "1.2.0",
  "private": true,
  "scripts": {
    "clean": "rimraf ./server/*.js ./server/**/*.js ./config/*.js ./config/**/*.js ./db/*.js ./db/**/*.js",
    "build": "run-s lint && run-p build:*",
    "build:client": "next build",
    "build:server": "run-s clean && tsc -p ./tsconfig.server.json",
    "dev": "TS_NODE_PROJECT=tsconfig.server.json DEBUG=CHEVCASTTV:* nodemon -w ./server -- ./server/server.ts",
    "lint": "next lint",
    "start": "NODE_ENV=production node ./server/server.js"
  },
  "dependencies": {
    "@chevtek/react-state-container": "^6.0.1",
    "@discordjs/opus": "^0.5.3",
    "@discordjs/voice": "^0.6.0",
    "@emotion/react": "^11.4.1",
    "@emotion/styled": "^11.3.0",
    "@google-cloud/text-to-speech": "^3.3.1",
    "@mui/icons-material": "^5.0.1",
    "@mui/material": "^5.0.0-rc.1",
    "@mui/system": "^5.0.1",
    "axios": "^0.21.4",
    "components": "file:components",
    "config": "file:config",
    "cookies": "^0.8.0",
    "db": "file:db",
    "debug": "^4.3.2",
    "discord.js": "^13.1.0",
    "dotenv": "^10.0.0",
    "dotenv-expand": "^5.1.0",
    "emoji-regex": "^10.0.0",
    "ffmpeg-static": "^4.4.0",
    "hooks": "file:hooks",
    "jsonwebtoken": "^8.5.1",
    "libsodium-wrappers": "^0.7.9",
    "moment": "^2.29.1",
    "mongodb": "^4.1.2",
    "mongoose": "^6.0.12",
    "next": "11.1.2",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-player": "^2.9.0",
    "styles": "file:styles",
    "tmi.js": "^1.8.5",
    "types": "file:types",
    "websocket": "^1.0.34"
  },
  "devDependencies": {
    "@types/cookies": "^0.7.7",
    "@types/debug": "^4.1.7",
    "@types/jsonwebtoken": "^8.5.5",
    "@types/react": "17.0.20",
    "@types/tmi.js": "^1.7.2",
    "@types/websocket": "^1.0.4",
    "eslint": "7.32.0",
    "eslint-config-next": "11.1.2",
    "nodemon": "^2.0.13",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "ts-node": "^10.2.1",
    "typescript": "^4.4.3"
  }
}
