{
  "name": "chevcasttv",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "clean": "rimraf ./server.js && rimraf ./config/index.js && rimraf ./db/index.js",
    "build": "npm-run-all -l clean lint -p build:*",
    "build:client": "next build",
    "build:server": "tsc -p ./tsconfig.server.json",
    "dev": "ts-node-dev --project ./tsconfig.server.json --ignore-watch ./ --watch ./server.ts,./config/*.ts,./db/*.ts -r dotenv/config ./server.ts",
    "lint": "next lint",
    "start": "NODE_ENV=production node -r dotenv/config ./build/server.js"
  },
  "dependencies": {
    "@emotion/react": "^11.4.1",
    "@emotion/styled": "^11.3.0",
    "@mui/icons-material": "^5.0.1",
    "@mui/material": "^5.0.0-rc.1",
    "@next-auth/mongodb-adapter": "0.0.2-next.281",
    "@types/debug": "^4.1.7",
    "axios": "^0.21.4",
    "components": "file:components",
    "config": "file:config",
    "db": "file:db",
    "debug": "^4.3.2",
    "dotenv": "^10.0.0",
    "mongodb": "^4.1.2",
    "next": "11.1.2",
    "next-auth": "^4.0.0-beta.2",
    "npm-run-all": "^4.1.5",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "types": "file:types"
  },
  "devDependencies": {
    "@types/react": "17.0.20",
    "eslint": "7.32.0",
    "eslint-config-next": "11.1.2",
    "rimraf": "^3.0.2",
    "ts-node-dev": "^1.1.8",
    "typescript": "4.4.3"
  }
}
