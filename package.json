{
  "name": "chevcasttv",
  "version": "1.2.0",
  "private": true,
  "scripts": {
    "clean": "rimraf ./server/*.js ./server/**/*.js ./config/*.js ./config/**/*.js ./db/*.js ./db/**/*.js ./types/*.js",
    "build": "run-s lint && run-p build:*",
    "build:client": "next build",
    "build:server": "run-s clean && tsc -p ./tsconfig.server.json",
    "dev": "TS_NODE_PROJECT=tsconfig.server.json DEBUG=CHEVCASTTV:* nodemon -w ./server -- ./server/server.ts",
    "lint": "next lint",
    "start": "NODE_ENV=production node ./server/server.js",
    "seeddb": "ts-node -P ./tsconfig.server.json ./db/seed.ts"
  },
  "dependencies": {
    "@chevtek/react-state-container": "^6.0.4",
    "@discordjs/opus": "^0.7.0",
    "@discordjs/voice": "^0.7.5",
    "@emotion/react": "^11.6.0",
    "@emotion/styled": "^11.6.0",
    "@google-cloud/text-to-speech": "^3.3.1",
    "@mui/icons-material": "^5.1.1",
    "@mui/material": "^5.1.1",
    "@mui/system": "^5.1.1",
    "axios": "^0.24.0",
    "components": "file:components",
    "config": "file:config",
    "cookies": "^0.8.0",
    "db": "file:db",
    "debug": "^4.3.2",
    "discord.js": "^13.3.1",
    "dotenv": "^10.0.0",
    "dotenv-expand": "^5.1.0",
    "emoji-regex": "^10.0.0",
    "ffmpeg-static": "^4.4.0",
    "hooks": "file:hooks",
    "immer": "^9.0.6",
    "jsonwebtoken": "^8.5.1",
    "libsodium-wrappers": "^0.7.9",
    "moment": "^2.29.1",
    "moment-timezone": "^0.5.34",
    "mongodb": "^4.2.0",
    "mongoose": "^6.0.13",
    "next": "12.0.4",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-player": "^2.9.0",
    "styles": "file:styles",
    "tmi.js": "^1.8.5",
    "types": "file:types",
    "websocket": "^1.0.34"
  },
  "devDependencies": {
    "@types/cookies": "^0.7.7",
    "@types/debug": "^4.1.7",
    "@types/jsonwebtoken": "^8.5.6",
    "@types/react": "17.0.35",
    "@types/tmi.js": "^1.8.0",
    "@types/websocket": "^1.0.4",
    "eslint": "7.32.0",
    "eslint-config-next": "12.0.4",
    "nodemon": "^2.0.15",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.2"
  }
}
