{
  "name": "chevcasttv",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "clean": "rimraf ./server/**/*.js && rimraf ./config/**/*.js && rimraf ./db/**/*.js",
    "build": "run-s lint && run-p build:*",
    "build:client": "next build",
    "build:server": "run-s clean && tsc -p ./tsconfig.server.json",
    "dev": "TS_NODE_PROJECT=tsconfig.server.json DEBUG=CHEVCASTTV:* nodemon ./server/server.ts",
    "lint": "next lint",
    "start": "NODE_ENV=production node ./server/server.js"
  },
  "dependencies": {
    "@discordjs/opus": "^0.5.3",
    "@discordjs/voice": "^0.6.0",
    "@emotion/react": "^11.4.1",
    "@emotion/styled": "^11.3.0",
    "@google-cloud/text-to-speech": "^3.3.1",
    "@mui/icons-material": "^5.0.1",
    "@mui/material": "^5.0.0-rc.1",
    "@mui/system": "^5.0.1",
    "@next-auth/mongodb-adapter": "^0.0.2-next.281",
    "axios": "^0.21.4",
    "components": "file:components",
    "config": "file:config",
    "db": "file:db",
    "debug": "^4.3.2",
    "discord.js": "^13.1.0",
    "dotenv": "^10.0.0",
    "dotenv-expand": "^5.1.0",
    "ffmpeg-static": "^4.4.0",
    "libsodium-wrappers": "^0.7.9",
    "mongodb": "^4.1.2",
    "next": "11.1.2",
    "next-auth": "^4.0.0-beta.4",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "styles": "file:styles",
    "tmi.js": "^1.8.5",
    "types": "file:types"
  },
  "devDependencies": {
    "@types/debug": "^4.1.7",
    "@types/react": "17.0.20",
    "@types/tmi.js": "^1.7.2",
    "eslint": "7.32.0",
    "eslint-config-next": "11.1.2",
    "nodemon": "^2.0.13",
    "rimraf": "^3.0.2",
    "typescript": "^4.4.3",
    "ts-node": "^10.2.1",
    "npm-run-all": "^4.1.5"
  }
}
