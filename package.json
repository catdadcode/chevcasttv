{
  "name": "chevcasttv",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "ts-node-dev --project ./server/tsconfig.json --ignore-watch ./ --watch ./server ./server/server.ts",
    "build": "npm run clean && next build && cd ./server && tsc",
    "start": "NODE_ENV=production node ./server/build/server.js",
    "clean": "rimraf ./server/build",
    "lint": "next lint"
  },
  "old-scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint"
  },
  "dependencies": {
    "@emotion/react": "^11.4.1",
    "@emotion/styled": "^11.3.0",
    "@mui/material": "^5.0.0-rc.1",
    "next": "11.1.2",
    "react": "17.0.2",
    "react-dom": "17.0.2"
  },
  "devDependencies": {
    "@types/react": "17.0.20",
    "eslint": "7.32.0",
    "eslint-config-next": "11.1.2",
    "rimraf": "^3.0.2",
    "ts-node-dev": "^1.1.8",
    "typescript": "4.4.3"
  }
}
